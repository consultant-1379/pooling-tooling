{
  "name": "rpt-express",
  "version": "1.2.97",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "start-dev": "export NODE_ENV=DEV || SET \"NODE_ENV=DEV\" && node server.js",
    "start-local-mongo-instance": "export NODE_ENV=DEV_LOCAL_MONGO || SET \"NODE_ENV=DEV_LOCAL_MONGO\" && nodemon server.js",
    "lint": "eslint -c .eslintrc.windows.js \"**/*.js\"",
    "lint-fix": "eslint --fix -c .eslintrc.windows.js \"**/*.js\"",
    "test-unit": "export NODE_ENV=TEST || SET \"NODE_ENV=TEST\" && mocha \"./{,!(node_modules)/**/}/unit/*.spec.js\"  --require config/config.js --exit",
    "test-integration": "export NODE_ENV=TEST || SET \"NODE_ENV=TEST\" && mocha \"./{,!(node_modules)/**/}/integration/*.spec.js\" --timeout 10000 --require config/config.js --exit",
    "test-unit-local-mongo-instance": "export NODE_ENV=TEST_LOCAL_MONGO || SET \"NODE_ENV=TEST_LOCAL_MONGO\" && mocha \"./{,!(node_modules)/**/}unit/*.spec.js\"  --require config/config.js --exit",
    "test-integration-local-mongo-instance": "export NODE_ENV=TEST_LOCAL_MONGO || SET \"NODE_ENV=TEST_LOCAL_MONGO\" && mocha \"./{,!(node_modules)/**/}integration/*.spec.js\" --timeout 10000 --require config/config.js --exit",
    "test-contract": "export NODE_ENV=TEST || SET \"NODE_ENV=TEST\" && mocha ./contract-tests/contract.spec.js --timeout 10000 --require config/config.js --exit",
    "test-contract-local-mongo-instance": "export NODE_ENV=TEST_LOCAL_MONGO || SET \"NODE_ENV=TEST_LOCAL_MONGO\" && mocha ./contract-tests/contract.spec.js --timeout 10000 --require config/config.js --exit",
    "cover:unit": "nyc --silent npm run test-unit",
    "cover:integration": "nyc --silent --no-clean npm run test-integration",
    "cover:merge": "nyc merge .nyc_output coverage.json",
    "cover:report": "nyc report --reporter=lcov"
  },
  "dependencies": {
    "bunyan": "1.8.15",
    "bunyan-elasticsearch": "1.0.1",
    "compare-versions": "4.1.1",
    "cors": "2.8.5",
    "cron-parser": "4.2.1",
    "croner": "8.0.2",
    "cuid": "2.1.8",
    "express": "4.17.1",
    "faker": "5.5.3",
    "generic-pool": "3.9.0",
    "jenkins": "0.28.1",
    "ldap-authentication": "2.2.9",
    "mongodb": "4.17.1",
    "nock": "13.0.11",
    "node-cron": "3.0.0",
    "nodemailer": "6.7.2",
    "passport": "0.5.3",
    "passport-custom": "1.1.1",
    "prom-client": "11.2.0",
    "response-time": "2.3.2",
    "semver": "7.3.7",
    "socket.io": "3.0.4",
    "superagent": "6.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "expect": "26.6.2",
    "mocha": "8.3.2",
    "newman": "5.3.2",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "sinon": "10.0.0"
  }
}
